/*initialization start*/
image{
  width:60rpx;
  height:60rpx;
}
/*initializaion end*/
.global{
  background-color: rgb(255,255,255);
  height:1250rpx;
  width:100vw;
  position: absolute;
}
.navigation-bar{
  width:100%;
  height:640rpx;
  background: linear-gradient(to right, #1AAD19, #1cc51c);
  position: absolute;
  z-index: 1;
}
.medal{
  display: block;
  width:74rpx;
  height:96rpx;
  position: absolute;
  left: 440rpx;
  top: 140rpx;
}
.setting{
  margin: 32rpx;
  margin-top: 10rpx;
  position: absolute;
  z-index: 1;
}
.userInfo{
  width:100%;
  height: 300rpx;
  margin:0 auto;
  margin-top:80rpx;
  position: absolute;
}
.circleFrame{
  width: 224rpx;
  height: 224rpx;
  background-color: white;
  border-radius: 50%;
  overflow:hidden;
  margin-left:auto;  
  margin-right:auto;  
}
.userName{
  margin-left:auto;
  margin-right:auto;
  margin-top:20rpx;
  text-align: center;
  font-size: 56rpx;
  color:#fff;
}
.userData{
  width: 100%;
  height: 120rpx;
  margin-top:30rpx; 
}
.checkingOutDays{
  width: 300rpx;
  height: 120rpx;
  display: inline-block;
  margin-left:74rpx;

}
.userGrade{
  width: 300rpx;
  height: 120rpx;
  display: inline-block;
  margin-right:74rpx;
}
.userData image{
  margin: 0rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
  vertical-align: middle;
}

.userData text{
  font-size: 28rpx;
  color: #FFF;
  display: inline-block;
  vertical-align: middle;
}

#timeunit{
  display: inline-block;
  width: 25rpx;
  height: 30rpx;
  font-size: 32rpx;
}
#datapackage{
  position: absolute;
  width: 100vw;
  z-index: 3;
  top:540rpx;
  }
.data{
  width: 680rpx;
  height: 200rpx;
  margin: 0 auto;
  background-color:#FFF; 
  border-radius:16rpx; 
  box-shadow: 0rpx 10rpx 20rpx 1rpx rgba(106, 145, 95, 0.3);
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  text-align: center;
  padding-bottom:12rpx;
}
.data view{
  width: 200rpx;
  height: 170rpx;
  margin: auto;
  font-size: 60rpx;
}
.data>view>.title{
  font-size: 28rpx;
  color:rgb(134, 134, 134);
}
#swiperpackage{
  width: 680rpx;
  height: 385rpx;
  margin-left:32rpx;
  position: absolute;
  top: 800rpx;
  border-radius:20rpx;
  overflow:hidden; 
  background-color: rgb(255, 255, 255);
  box-shadow: 0rpx 10rpx 26rpx 2rpx rgb(187, 187, 187);
}
.swiper{
  width: 680rpx;
  height: 385rpx;
}
.plan{
  width: 680rpx;
  height: 385rpx;
  background-color: rgb(3, 36, 3);
}
.rank{
  width: 680rpx;
  height: 385rpx;
  background-color: rgb(134, 26, 206);
}
.note{
  width: 680rpx;
  height: 385rpx;
  background-color: rgb(255, 255, 255)
}
.list{
  width: 680rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.topbar{
  width: 680rpx;
  height: 86rpx;
  font-size: 40rpx;
  background-color: #FFCC00;
  text-align:center;
  border-top: #000 1rpx;
  color: #fff;
  line-height: 86rpx;
}
.thing{
  width: 600rpx;
  margin:auto;
  margin-bottom:0;
  border-bottom:1rpx solid rgb(218, 216, 216); 
  font-size: 34rpx;
}

.thing-bottom{
  margin-bottom:40rpx;
}

.thing text, .thing image{
  vertical-align:middle;
}

.thing .note{
  display:inline-block;
  width:415rpx;
  height:44rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}

.thing .time{
  margin-left:20rpx;
}

.thing .clock{
  margin-left:10rpx;
}